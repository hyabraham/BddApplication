<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bt.be.bddapplication">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Controler.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Controler.SubscribtionActivity" />
        <activity android:name=".Controler.HomeActivity" />
        <activity android:name=".Controler.StatActivity" />
        <activity android:name=".Controler.CreateProviderActivity" />
        <activity android:name=".Controler.CreateFrigoActivity" />
        <activity android:name=".Controler.CreateProductActivity" />
        <activity android:name=".Controler.MouvementStockActivity" />
        <activity android:name=".Controler.ConfigActivity" />
        <activity android:name=".Controler.CheckStockActivity"></activity>
    </application>

</manifest>